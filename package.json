{
  "name": "e-commerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "postinstall": "prisma generate",
    "build": "rimraf .next && cross-env NODE_OPTIONS='--trace-warnings' next build",
    "start": "next start",
    "lint": "next lint",
    "dev": "set NEXT_DISABLE_TURBOPACK=1 && set NEXT_TELEMETRY_DISABLED=1 && prisma generate && next dev",
